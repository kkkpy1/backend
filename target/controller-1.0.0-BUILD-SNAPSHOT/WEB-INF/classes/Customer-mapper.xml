<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Customer">

	<parameterMap type="customerVO" id="insertParameterMap">
		<parameter property="userid" javaType="String" jdbcType="VARCHAR" mode="IN"/>
		<parameter property="passwd" javaType="String" jdbcType="VARCHAR" mode="IN"/>
		<parameter property="name" javaType="String" jdbcType="VARCHAR" mode="IN"/>
		<parameter property="gender" javaType="String" jdbcType="VARCHAR" mode="IN"/>
		<parameter property="email" javaType="String" jdbcType="VARCHAR" mode="IN"/>
		<parameter property="phone" javaType="String" jdbcType="VARCHAR" mode="IN"/>
		<parameter property="terms" javaType="String" jdbcType="CHAR" mode="IN"/>
	</parameterMap>
	
	<insert id="insertSP" parameterType="customerVO" 
			statementType="CALLABLE" parameterMap="insertParameterMap">
		{ call sp_customer_insert(?, ?, ?, ?, ?, ?, ?) }
	</insert>
	
</mapper>